            "GS\\GenericParts\\Tests\\": "tests/"
        }
    },
	"version":		"0.0.0",
    "type":			"symfony-bundle",
    "description":	"Basic components of Symfony application for a web project.",
    "authors":		[
        {
            "name":			"Grigory Koblitsky",
			"email":		"son5-29@mail.ru"
        }
    ],
    "keywords":		["green-symfony", "general-parts", "symfony-ux"],
    "license":		"MIT",
    "require":		{
        "php": ">=7",
		"fzaninotto/faker": "^1",
		"nesbot/carbon": "^2|^1",
        "symfony/filesystem": "^7|^6|^5|^4",
        "symfony/yaml": "^7|^6|^5|^4",
        "symfony/http-foundation": "^7|^6|^5|^4",
        "doctrine/migrations": "^3|^2",
        "symfony/asset": "^7|^6|^5|^4",
        "symfony/console": "^7|^6|^5|^4",
        "symfony/dependency-injection": "^6|^5|^4",
        "symfony/config": "^7|^6|^5|^4",
        "symfony/polyfill-intl-grapheme": "^1.27",
        "symfony/deprecation-contracts": "^3|^2",
        "symfony/error-handler": "^7|^6|^5|^4",
        "symfony/event-dispatcher-contracts": "^3|^2",
        "doctrine/annotations": "^3|^2",
        "nyholm/psr7": "^1.5",
