        "twig/intl-extra": "^3|^2|^1",
        "symfony/twig-bridge": "^6|^5|^4|^3",
        "symfony/security-core": "^6|^5|^4",
        "symfony/security-bundle": "^6|^5|^4",
        "symfony/property-access": "^6|^5|^4|^3"
