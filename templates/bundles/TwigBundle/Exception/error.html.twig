{% extends 'bundles/TwigBundle/Exception/base.error.html.twig' %}

{% block body %}
	<div>
		{{ 'error_controller_name: "' ~ (controller_name ?? '?') ~ '"' }}
	</div>

	<div>
		{{ 'message: "' ~ (exception.message ?? '?') ~ '"' }}
	</div>
	<div>
		{{ 'status_code: "' ~ (status_code ?? '?') ~ '"' }}
	</div>
	<div>
		{{ 'status_text: "' ~ (status_text ?? '?') ~ '"' }}
	</div>
	<div>
		{{ 'trace: "' ~ (exception.traceAsString ?? '?') ~ '"' }}
	</div>
	<div>
		{{ 'count_errors: "' ~ (count_errors ?? '?') ~ '"' }}
	</div>
{% endblock body %}